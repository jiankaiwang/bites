            <div class="modal-body">  
                <div class="container-fluid">
                    <div class="row col-md-12 col-lg-12 section-row">
                        <div class="col-md-6"><i class="fa fa-calendar"></i> <%= firstVaccine %></div>
                        <div class="col-md-6">
