    <!-- logo -->
    <div class="mark"><img class="logo" src='img/bites_logo_min.png'/></div>

    <!-- nav -->
    <div id="nav">
      <div class="container-fluid">
        <div class="row">
          <div class="col-xs-10 col-md-10">
            <h3><div class="meau-mark"><img class="logo" src='img/bites_logo_min.png'/></div></h3>
          </div>
